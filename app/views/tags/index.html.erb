<h1>All Tags</h1>

<ul id="tags">
  <% @tags.each do |tag| %>
    <li>
      <%= link_to tag, tag_path(tag) %>
      <%= link_to "delete", 
        tag_path(tag), 
        method: :delete, 
        data: {confirm: "Really delete this tag?"} %>
<br />
    </li>
  <% end %>
</ul>
<hr />
<%= link_to "<< Back to Articles", articles_path %>
